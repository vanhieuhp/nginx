;
; BIND data file for local loopback interface
;
$TTL    604800
@       IN      SOA     ns.main.com. hostmaster.main.com.(
                            202         ; Serial
                            600         ; Refresh
                           3600        ; Retry
                        2419200)       ; Expire
;

@       IN      NS      ns.main.com.
ns      IN      A       127.0.0.1
